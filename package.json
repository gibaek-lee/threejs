{
  "name": "threejs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "start": "nuxt start",
    "dev": "npm run copy:lib_modules && npm run compile:ts_cannon && nuxt",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "compile:ts_cannon": "tsc static/cannon/business/*.ts",
    "copy:lib_modules": "cp -R node_modules/cannon/build static/cannon && cp -R node_modules/three/examples/js/libs/draco static",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.24.6",
    "cannon": "^0.6.2",
    "core-js": "^3.15.1",
    "dat.gui": "^0.7.7",
    "nuxt": "^2.15.7",
    "three": "^0.130.1",
    "vuetify": "^2.5.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.1",
    "@types/cannon": "^0.1.7",
    "@types/dat.gui": "^0.7.7",
    "@types/three": "^0.130.0",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^27.0.5",
    "push-dir": "^0.4.1",
    "ts-jest": "^27.0.3",
    "vue-jest": "^3.0.4"
  }
}
